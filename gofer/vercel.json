{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1",
        "headers": {
          "Cache-Control": "s-maxage=3600, stale-while-revalidate"
        }
      }
    ]
  }